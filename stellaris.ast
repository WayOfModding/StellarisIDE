script
	:	line+
	;

line
	:	comment
	|	pair comment?
	;

comment
	:	'#' .+ $
	;

pairs
	:	pair+
	;

pair
	:	key = value
	;

value
	:	INTEGER
	|	FLOAT
	|	STRING
	|	{ pairs }
	;

key
	:	ud_str
	;

INTEGER
	:	DIGIT+
	;

FLOAT
	:	'-'? INTEGER '.' INTEGER
	;

STRING
	:	ud_str
	|	'"' ud_str '"'
	;

ud_str
	:	CHARACTER+
	;